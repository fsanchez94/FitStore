# Django Settings
SECRET_KEY=your-secret-key-here-generate-a-long-random-string
DEBUG=False

# Allowed Hosts (comma-separated)
ALLOWED_HOSTS=your-app.up.railway.app

# Database - Supabase PostgreSQL Connection String
